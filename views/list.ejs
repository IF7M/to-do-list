
<%- include("header") -%>

  <div class="box" id="heading">
    <h1><%=dayIs%></h1>
  </div>
  <div class="box ">



    <% for (var i=0; i<newListItems.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p><%= newListItems[i] %></p>
    </div>

    <% } %>




    <form class="item" action="/" method="post">
      <input type="text" name="newTask" placeholder="Add New Task" autocomplete="off">
      <button type="submit" name="button" value="adding">+</button>
      <button type="submit" name="button" value="remove">-</button>

    </form>

  </div>

<%- include("footer") -%>
